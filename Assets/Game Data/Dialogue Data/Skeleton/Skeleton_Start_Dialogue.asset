  - ID: list1
    dialoguePieces:
    - image: {fileID: 0}
      text: "\u7ADF\u7136\u6709\u4EBA\u80FD\u591F\u4ECE\u7262\u623F\u91CC\u9003\u51FA\u6765\uFF0C\u8FD9\u53EF\u771F\u662F\u5C11\u89C1\u3002"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u662F\u4E0D\u662F\u611F\u89C9\u5F88\u5947\u602A\uFF0C\u4E3A\u4EC0\u4E48\u6211\u53EA\u5269\u9AA8\u5934\u4E86\u5374\u8FD8\u80FD\u8BF4\u8BDD\u3002"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u6211\u4EEC\u6240\u5728\u7684\u8FD9\u4E2A\u57CE\u5821\uFF0C\u5145\u6EE1\u7740\u672A\u77E5\u5730\u9B54\u6CD5\u6C14\u606F\u3002"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u6B63\u56E0\u4E3A\u8FD9\u91CC\u5145\u76C8\u7684\u9B54\u6CD5\u6C14\u606F\uFF0C\u624D\u4F7F\u5F97\u6211\u80FD\u591F\u4EE5\u8FD9\u79CD\u65B9\u5F0F\u6D3B\u4E0B\u6765\u3002"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u53EF\u662F\u8FD9\u80A1\u9B54\u6CD5\u6C14\u606F\u8C8C\u4F3C\u5BF9\u6D3B\u7740\u7684\u4EBA\u6709\u5BB3\u3002"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u57CE\u5821\u5185\u5927\u90E8\u5206\u4EBA\uFF0C\u90FD\u56E0\u4E3A\u8FD9\u80A1\u9B54\u6CD5\u6C14\u606F\u53D1\u4E86\u75AF\uFF0C\u4ED6\u4EEC\u4E0D\u5206\u654C\u53CB\u5730\u653B\u51FB\uFF0C\u76F4\u81F3\u81EA\u5DF1\u6B7B\u4EA1"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u53EA\u6709\u5C11\u6570\u4EBA\u80FD\u591F\u514D\u53D7\u9B54\u6CD5\u7684\u4FB5\u8680\uFF0C\u4FDD\u6301\u7406\u667A\uFF0C\u4ECE\u800C\u5E78\u514D\u4E8E\u96BE"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u6211\u4EE5\u8FD9\u79CD\u72B6\u6001\u6E05\u9192\u8FC7\u6765\u540E\uFF0C\u88AB\u8FD9\u91CC\u7684\u5546\u5E97\u8001\u677F\u6240\u6551"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u662F\u5979\u5E26\u6211\u5230\u8FD9\u7247\u72EC\u7ACB\u4E8E\u57CE\u5821\u7684\u7A7A\u95F4\u907F\u96BE"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u867D\u7136\u6211\u73B0\u5728\u4E5F\u4E0D\u4F1A\u518D\u6B7B\u53BB\u4E86\u3002\u3002"
      haveEvent: 0

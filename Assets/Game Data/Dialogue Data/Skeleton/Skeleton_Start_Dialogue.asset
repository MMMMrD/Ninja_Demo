      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u8FD9\u6837\u770B\u6765\uFF0C\u53EA\u6709\u4F60\u6709\u80FD\u529B\u6E05\u9664\u8FD9\u4E2A\u57CE\u5821\u4E2D\u7684\u4E0D\u660E\u9B54\u6CD5\u6C14\u606F\uFF0C\u89E3\u6551\u57CE\u5821\u4E2D\u7684\u4EBA\u4EEC"

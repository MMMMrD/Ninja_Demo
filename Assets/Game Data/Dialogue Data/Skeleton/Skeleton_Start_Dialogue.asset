      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u6211\u5E0C\u671B\u4F60\u80FD\u6E05\u9664\u8FD9\u4E2A\u57CE\u5821\u4E2D\u7684\u8FD9\u80A1\u8BE1\u5F02\u7684\u9B54\u6CD5\u6C14\u606F"
      haveEvent: 0
      questData: {fileID: 0}
      dialogueOptions: []
    - image: {fileID: 0}
      text: "\u4F60\u80FD\u591F\u4ECE\u5730\u7262\u4E2D\u9003\u751F\uFF0C\u53C8\u80FD\u591F\u5728\u8FD9\u80A1\u8BE1\u5F02\u7684\u9B54\u6CD5\u6C14\u606F\u4E2D\u4FDD\u6301\u7406\u667A"
